{
  "manifest_version": 3,
  "name": "Pupa",
  "description": "Pupa and Lupa",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_matches": [
        "*://pupa-and-lupa.web.app/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
